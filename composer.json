    "license": "MIT",
    "description": "Lasso - Asset wrangling for Laravel made simple.",
    "type": "library",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Sam Carr√©",
            "email": "sam.carre2000@gmail.com"
        }
